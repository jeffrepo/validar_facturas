<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_inh" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inh1</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="account.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='group_analytic_accounting']/parent::div/parent::div[hasclass('o_setting_box')]/parent::div[hasclass('o_settings_container')]" position="after">
                          <h2>Blacklist SAT</h2>
                          <div class="row mt16 o_settings_container" >
                               <div class="col-12 col-lg-6 o_setting_box">
                                    <div class="o_setting_right_pane">
                                         <div class="content-group">
                                             <div class="mt16">   
                                                  <label for="url_blacklist_sat" class="o_light_label"/>
                                                  <field name="url_blacklist_sat" />
                                             </div>
                                          </div>
                                    </div>
                                </div>
                           </div>
                       


                </xpath>
           </field>
        </record>
   </data>
</odoo>
